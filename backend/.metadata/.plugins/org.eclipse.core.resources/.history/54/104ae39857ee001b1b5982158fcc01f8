package br.com.adriano.tcc.order.repository;

import java.util.List;
import java.util.Optional;

import org.springframework.data.jpa.repository.JpaRepository;

import br.com.adriano.tcc.order.model.AddressModel;

public interface OrderRepository extends JpaRepository<AddressModel, Long> {
	
	Optional<List<AddressModel>> findAllByUserId(Long userId);

}
